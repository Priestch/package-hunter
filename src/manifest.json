{
  "manifest_version": 2,
  "name": "Package Hunter",
  "description": "A browser extension to help programmer find different programming language packages effectively.",
  "version": "1.2.1",
  "content_scripts": [
    {
      "matches": ["*://pypi.org/search/?*", "*://crates.io/search*"],
      "css": ["assets/styles.css"],
      "js": ["index.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://pypi.org/*",
    "https://github.com/*"
  ],
  "key": ""
}
